{
  "rootDir": "../../",
  "moduleDirectories": [
    "<rootDir>/src",
    "<rootDir>/node_modules"
  ],
  "testRegex": "(/__tests__/.*\\.(test))\\.(ts|js)$",
  "setupTestFrameworkScriptFile": "<rootDir>/config/jest/setup.js",
  "moduleFileExtensions": [
    "js",
    "ts",
    "json"
  ],
  "transform": {
    "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
  }
}
